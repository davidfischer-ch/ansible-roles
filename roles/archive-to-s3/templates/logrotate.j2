{{ archive_to_s3_log_directory }}/*.log {
       {{ archive_to_s3_log_rotations_frequency }}
       dateext
       missingok
       rotate {{ archive_to_s3_log_rotations_max_count|int }}
       compress
       delaycompress
       nomail
       notifempty
       create 644 root root
       sharedscripts
}
